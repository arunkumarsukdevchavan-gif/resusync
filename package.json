{
  "name": "resusync-monorepo",
  "version": "1.0.0",
  "description": "ResuSync - AI-powered resume optimization platform",
  "scripts": {
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install", 
    "install:all": "npm run install:backend && npm run install:frontend",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm run dev",
    "start:both": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start": "powershell -ExecutionPolicy Bypass -File start-resusync.ps1",
    "quick-start": "start-resusync.bat"
  },
  "keywords": [
    "resume",
    "ai",
    "optimization",
    "job-matching"
  ],
  "author": "Arun Kumar",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.0"
  }
}
